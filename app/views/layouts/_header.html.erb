    <header>
      <nav>
        <%= link_to 'TestGuru', root_path, class: 'nav logo' %>
        <% if logged_in? %>
          <p class="nav user">
            Welcome, <b><%= current_user.email %></b> Guru |
            <%= link_to 'Log out', logout_path, method: :delete %>
          </p>
        <% else %>
          <span class="nav link">
            <%= link_to 'SignUp', signup_path %> |
            <%= link_to 'Login', login_path %>
          </span>
        <% end %>
      </nav>
      <hr>
     </p>
    </header>